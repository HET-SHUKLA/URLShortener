meta {
  name: Register with Email
  type: http
  seq: 1
}

post {
  url: {{DOMAIN}}/{{API}}/{{VERSION}}/{{PATH}}/register
  body: json
  auth: inherit
}

headers {
  x-client-type: web
}

body:json {
  {
    "email": "abc@gmail.com",
    "password": "12345678"
  }
}

body:form-urlencoded {
  ~email: abc@gmail.com
  ~password: 1234
}

settings {
  encodeUrl: true
  timeout: 0
}
